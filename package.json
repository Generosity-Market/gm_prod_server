{
    "name": "gm_prod_server",
    "version": "1.0.0",
    "author": "Joseph Gordy",
    "description": "",
    "main": "server.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "dev": "PORT=3002 nodemon server.js",
        "start": "nodemon server.js",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
        "seed:db": "npx sequelize-cli db:seed:all",
        "lint": "eslint ./",
        "lint-staged": "lint-staged",
        "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
        "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
        "pretest:ci": "npm run lint && cross-env NODE_ENV=ci npm run migrate:reset",
        "test:ci": "cross-env NODE_ENV=ci jest --verbose --testTimeout=10000 --colors",
        "test:watch": "npm run test -- --verbose --watch",
        "test:coverage": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
        "prepare": "husky install"
    },
    "keywords": [
        "node",
        "express",
        "stripe",
        "aws"
    ],
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.1074.0",
        "bcrypt": "^5.0.1",
        "bluebird": "^3.7.2",
        "body-parser": "^1.19.1",
        "chalk": "^4.1.2",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "file-type": "^17.1.1",
        "fs": "0.0.1-security",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multiparty": "^4.2.3",
        "node-fetch": "^3.2.0",
        "nodemailer": "^6.7.2",
        "nodemon": "^2.0.20",
        "passport": "^0.5.2",
        "passport-http": "^0.3.0",
        "passport-jwt": "^4.0.0",
        "path": "^0.12.7",
        "pg": "^8.7.3",
        "sequelize": "^6.16.1",
        "sequelize-cli": "^6.4.1",
        "stripe": "^8.203.0",
        "supertest": "^6.2.2",
        "swagger-ui-express": "^4.3.0"
    },
    "devDependencies": {
        "coveralls": "^3.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.9.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.25.4",
        "husky": "^7.0.0",
        "jest": "^27.5.1",
        "jest-cli": "^27.5.1",
        "lint-staged": "^12.3.4"
    }
}